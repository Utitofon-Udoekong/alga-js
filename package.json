{
  "name": "alga-js",
  "version": "0.0.6",
  "description": "Alga.js is a JavaScript helper to help build UI components by using any of modern web frameworks or libraries",
  "module": "dist/alga.min.js",
  "main": "dist/alga-umd.js",
  "exports": {
    ".": {
      "import": "dist/alga.min.js",
      "require": "dist/alga-umd.js"
    }
  },
  "unpkg": "dist/alga-umd.js",
  "typings": "types/app.d.ts",
  "files": [
    "js",
    "dist/alga.min.js",
    "dist/alga-umd.js",
    "lib/*.js",
    "types/**/*.d.ts"
  ],
  "scripts": {
    "build": "rollup --config",
    "test": "jest",
    "lint": "eslint js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedirghazali/alga-js.git"
  },
  "keywords": [
    "alga-js",
    "framework",
    "front-end",
    "utilities"
  ],
  "author": "Tedir Ghazali",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/tedirghazali/alga-js/issues"
  },
  "homepage": "https://github.com/tedirghazali/alga-js#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
